+                        break;
+                    case 4:
+                        pokemon5.setImage(image);
+                        break;
+                    case 5:
+                        pokemon6.setImage(image);
+                        break;
+                }
+            }
+
+            // Cerrar la conexión y liberar los recursos
+            resultSet.close();
+            statement.close();
+            connection.close();
+        } catch (SQLException e) {
+            e.printStackTrace();
+            // Maneja el error apropiadamente
+        }
+    }
