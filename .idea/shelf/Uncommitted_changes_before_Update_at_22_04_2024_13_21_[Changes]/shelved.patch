+            // Obtener la conexión a la base de datos
+            Connection connection = DBConnection.getConnection();
+
+            // Consulta para obtener los Pokémon que están en la caja (con CAJA = 1)
+            String sql = "SELECT * FROM pokemon WHERE CAJA = 1";
+            PreparedStatement statement = connection.prepareStatement(sql);
+            ResultSet resultSet = statement.executeQuery();
+
+            // Lista para almacenar los Pokémon
+            List<Pokemon> pokemonList = new ArrayList<>();
+
+            // Iterar sobre los resultados de la consulta y agregar los Pokémon a la lista
+            while (resultSet.next()) {
+                int numPokedex = resultSet.getInt("NUM_POKEDEX");
+                // Obtener la ruta de la imagen del Pokémon desde la tabla pokedex
+                String imagePath = getPokemonImagePath(numPokedex);
+
+                // Crear un objeto Pokemon y agregarlo a la lista
+                Pokemon pokemon = new Pokemon(numPokedex, imagePath);
+                pokemonList.add(pokemon);
+            }
+
+            // Asignar las imágenes de los Pokémon a las ImageView correspondientes
+            for (int i = 0; i < pokemonList.size(); i++) {
+                Pokemon pokemon = pokemonList.get(i);
+                String imagePath = pokemon.getImagePath();
+
+                // Crear un objeto Image a partir de la ruta de la imagen
+                Image image = new Image(imagePath);
+
+                // Asignar la imagen a la ImageView correspondiente
+                switch (i) {
+                    case 0:
+                        pokemon1.setImage(image);
+                        break;
+                    case 1:
+                        pokemon2.setImage(image);
+                        break;
